{
  "name": "@natlibfi/marc-merge-ui",
  "description": "UI for merging MARC records",
  "version": "1.0.0",
  "author": {
    "name": "The National Library of Finland"
  },
  "keywords": [
    "marc",
    "merge"
  ],
  "homepage": "https://github.com/natlibfi/marc-merge-ui",
  "bugs": {
    "url": "https://github.com/natlibfi/marc-merge-ui/issues"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:natlibfi/marc-merge-ui.git"
  },
  "main": "index.js",
  "scripts": {
    "clean:build": "node ./bin/clean.js createdir",
    "build:html": "node ./bin/buildHtml.js",
    "start": "NODE_PATH=build/commons node build/index.js",
    "prebuild": "npm run clean:build && npm run build:html",
    "build": "npm run build:server && npm run build:commons && npm run build:frontend",
    "build:frontend": "export NODE_ENV=production & NODE_ENV=production & webpack -p --config ./config/webpack.config.prod.js --progress",
    "build:server": "cd server && BABEL_ENV=production babel . --out-dir ../build --source-maps",
    "build:commons": "babel melinda-ui-commons --ignore node_modules,spec.js --out-dir build/commons --source-maps",
    "test": "npm run lint && npm run test:frontend && npm run test:server",
    "test:frontend": "BABEL_ENV=test mocha --compilers js:babel-core/register --require ./frontend/test/test_bootstrap.js \"frontend/**/*@(.spec.js|.spec.jsx)\"",
    "test:frontend:watch": "npm run test:frontend -- --watch --watch-extensions story,stories",
    "test:server": "BABEL_ENV=test mocha --compilers js:babel-core/register --require ./server/tests/test_bootstrap.js --require ./melinda-ui-commons/server/test_helpers/test_bootstrap.js \"./server/tests\"  \"./melinda-ui-commons/server/**/*@(.spec.js|.spec.jsx)\"",
    "test:server:watch": "npm run test:server -- --watch",
    "lint": "eslint --ext .jsx,.js .",
    "dev": "node ./bin/clean.js && npm run watch",
    "watch": "npm run watch:ui & npm run watch:server",
    "watch:ui": "DEBUG=1 webpack-dev-server --config ./config/webpack.config.dev.js --hot --inline --progres",
    "watch:server": "NODE_ENV=debug nodemon --watch server --exec babel-node --presets=es2015 server/index.js",
    "cli": "babel-node ./bin/cli.js",
    "publish-to-npm": "npm run publish --access=public"
  },
  "license": "AGPL-3.0+",
  "dependencies": {
    "@natlibfi/marc-record-merge": "^3.3.0",
    "@natlibfi/melinda-api-client": "^1.1.4",
    "@natlibfi/melinda-ui-commons": "file:melinda-ui-commons",
    "archiver": "^1.1.0",
    "babel-eslint": "^7.1.0",
    "body-parser": "^1.15.2",
    "classnames": "^2.2.5",
    "cookie-parser": "^1.4.3",
    "cors": "^2.8.0",
    "es6-promise": "^3.2.1",
    "es6-promisify": "^4.1.0",
    "eslint": "^3.10.1",
    "express": "^4.14.0",
    "express-winston": "^2.0.0",
    "fbjs": "^0.8.6",
    "http-status-codes": "^1.0.6",
    "immutable": "^3.8.1",
    "isomorphic-fetch": "^2.2.1",
    "lodash": "^4.15.0",
    "marc-record-js": "^0.3.1",
    "mkdirp": "^0.5.1",
    "moment": "^2.15.1",
    "object-assign": "^4.1.0",
    "uuid": "^3.0.1",
    "winston": "^2.2.0",
    "xml2js": "^0.4.17"
  },
  "devDependencies": {
    "autoprefixer": "^6.4.0",
    "babel-cli": "^6.14.0",
    "babel-eslint": "^6.1.2",
    "babel-loader": "^6.2.5",
    "babel-plugin-module-resolver": "^2.3.0",
    "babel-plugin-rewire": "^1.0.0-rc-7",
    "babel-polyfill": "^6.13.0",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-2": "^6.13.0",
    "chai": "^3.5.0",
    "chai-immutable": "^1.6.0",
    "cheerio": "^0.22.0",
    "colors": "^1.1.2",
    "columnify": "^1.5.4",
    "copy-webpack-plugin": "^3.0.1",
    "css-loader": "^0.24.0",
    "eslint": "^3.3.1",
    "eslint-config-airbnb": "^10.0.1",
    "eslint-plugin-import": "^1.14.0",
    "eslint-plugin-jsx-a11y": "^2.1.0",
    "eslint-plugin-mocha": "^4.7.0",
    "eslint-plugin-react": "^6.1.2",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "iso9_1995": "0.0.2",
    "js-cookie": "^2.1.3",
    "jsdom": "^9.4.2",
    "json-loader": "^0.5.4",
    "json-similarity": "https://github.com/NatLibFi/json-similarity.git#c70cb1ac3270143440c6450463e4ffdee2a15ce2",
    "mocha": "^3.0.2",
    "node-sass": "^3.8.0",
    "node-uuid": "^1.4.7",
    "nodemon": "^1.10.2",
    "postcss-loader": "^0.10.1",
    "q": "^1.4.1",
    "react": "^15.3.1",
    "react-dnd": "^2.1.4",
    "react-dnd-html5-backend": "^2.1.2",
    "react-dom": "^15.3.1",
    "react-hot-loader": "^1.3.0",
    "react-redux": "^4.4.5",
    "react-router": "^2.7.0",
    "redux": "^3.5.2",
    "redux-immutable": "^3.0.8",
    "redux-logger": "^2.6.1",
    "redux-thunk": "^2.1.0",
    "reselect": "^2.5.4",
    "rimraf": "^2.5.4",
    "sass-loader": "^4.0.0",
    "sfs4900": "0.0.1",
    "sinon": "^1.17.5",
    "sinon-as-promised": "^4.0.2",
    "sinon-chai": "^2.8.0",
    "sinon-stub-promise": "^3.0.1",
    "style-loader": "^0.13.1",
    "supertest": "^2.0.0",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.2",
    "webpack-dev-server": "^1.15.0",
    "xregexp": "^3.1.1"
  }
}
